*************************************************************************
#include <stdio.h>
int add(){
    int a,b;
    scanf("%d %d",&a,&b);
    return printf("%d",a+b);
}
int subtract(){
    int a,b;
    scanf("%d %d",&a,&b);
    return printf("%d",a-b);
}
int divide(){
    int a,b;
    float c;
    scanf("%d %d",&a,&b);
    return printf("%f",c=a/b);
}
int remainde(){
    int a,b;
    float c;
    scanf("%d %d",&a,&b);
    return printf("%f",c=a%b);
}
int main(){
    add();
    subtract();
    divide();
    remainde();
}
