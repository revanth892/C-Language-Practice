    long int Mobile;
    Mobile=66666666666;
    printf("Name: rfdfdf \n");
    printf("Birth Date: 25 May \n");
    printf("Mobile No %ld\n",Mobile);
}

#include <stdio.h>
#include <string.h>

    
int main(){
    char char1='X';
    char char2='M';
    char char3='L';
    printf("The reverse order of %c %c %c is %c %c %c",char1,char2,char3,char3,char2,char1);
}
    
#include <stdio.h>
#include <string.h>
void perimeterandare(){
    int a,b;
    printf("Give input for the sides of Rectangle\n");
    scanf("\n %d \n%d",&a,&b);
    printf("Perimeter Of Rectangle is %d\nArea Of Rectangle is %d",2*(a+b),a*b);
};
    
int main(){
    perimeterandare();
}


#include <stdio.h>
#include <string.h>
void perimeterandare(){
    float pi=3.14;
    int r;
    printf("Give input for the radius of circle\n");
    scanf("%d",&r);
    printf("Perimeter Of circle is %f\nArea Of circle is %f",2*pi*r,pi*r*r);
};
    
int main(){
    perimeterandare();
}

#include <stdio.h>
#include <string.h>
void addacctogivenintsrtuctions(){
    int a,b;
    a=125;
    b=12345;
    long int ax=1234567890;
    short int s=4043;
    float x=2.13459;
    double dx =1.415927;
    char c='W';
    unsigned long ux = 2541567890;
    printf("a+c:%d\n",a+c);
    printf("x+c:%f\n",x+c);
    printf("dx+c:%f\n",dx+x);
    printf("((int)dx)+ax:%ld\n",((int)dx)+ax);
    printf("a+x:%f\n",a+x);
    printf("s+b:%d\n",s+b);
    printf("ax+b:%ld\n",ax+b);
    printf("s+c:%hd\n",s+c);
    printf("ax+c%ld\n",ax+c);
    printf("ax+ux:%ld\n",ax+ux);
};
    
int main(){
    addacctogivenintsrtuctions();
}


#include <stdio.h>
#include <string.h>
void calculateyearsweeksdays(){
    int inputdays =1329;
    int years=inputdays/365;
    int remainingdays=1329-(365*years);
    int weeks = remainingdays/7;
    int fewdays=remainingdays-(weeks*7);
    printf("Years:%d\nWeeks:%d\nDays:%d",years,weeks,fewdays);
    
};
    
int main(){
    calculateyearsweeksdays();
}
    

#include <stdio.h>
#include <string.h>
int userinputadd(){
    int a,b;
    printf("Enter the numbers:\n");
    scanf("%d%d",&a,&b);
    return printf("Sum:%d",a+b);
    
};
    
int main(){
    userinputadd();
}


